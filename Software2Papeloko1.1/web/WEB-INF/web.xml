<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>Faces Servlet</servlet-name>
        <servlet-class>javax.faces.webapp.FacesServlet</servlet-class>
        <load-on-startup>1</load-on-startup>
    </servlet>
    
    <servlet>
        <servlet-name>PeticionesJsUsuarios</servlet-name>
        <servlet-class>Modelo.PeticionesJs.PeticionesJsUsuarios</servlet-class>
    </servlet>
    
    <servlet>
        <servlet-name>PeticionesJsCategorias</servlet-name>
        <servlet-class>Modelo.PeticionesJs.PeticionesJsCategorias</servlet-class>
    </servlet>
    
    <servlet>
        <servlet-name>PeticionesJsProveedores</servlet-name>
        <servlet-class>Modelo.PeticionesJs.PeticionesJsProveedores</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>PeticionesJsStock</servlet-name>
        <servlet-class>Modelo.PeticionesJs.PeticionesJsStock</servlet-class>
    </servlet>
        
    <servlet-mapping>
        <servlet-name>PeticionesJsUsuarios</servlet-name>
        <url-pattern>/PeticionesJsUsuarios</url-pattern>
    </servlet-mapping>
    
    <servlet-mapping>
        <servlet-name>PeticionesJsCategorias</servlet-name>
        <url-pattern>/PeticionesJsCategorias</url-pattern>
    </servlet-mapping>
    
    <servlet-mapping>
        <servlet-name>PeticionesJsProveedores</servlet-name>
        <url-pattern>/PeticionesJsProveedores</url-pattern>
    </servlet-mapping>
    
    <servlet-mapping>
        <servlet-name>PeticionesJsStock</servlet-name>
        <url-pattern>/PeticionesJsStock</url-pattern>
    </servlet-mapping>
    
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>/Vista/VistaRegistros/GestionUsuarios/</welcome-file>
    </welcome-file-list>
</web-app>
